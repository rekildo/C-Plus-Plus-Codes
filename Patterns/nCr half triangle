#include<iostream>
using namespace std;

int fact(int num) {
	int res = 1;

	for(int i = 1; i<=num; i++) {
		res = res * i;
	}
	return res;
}


int nCr(int n, int r) {
	int factn = fact(n);
	int factr = fact(r);
	int factn_r = fact(n-r);

	return  factn/(factr*factn_r);
}


//nCr
int main() 
{	
	int n;
	cin>>n;

	for(int i = 0; i<=n; i++) {
		for(int j = 0; j<=i; j++) {
			cout<<nCr(i,j)<<" ";
		}
		cout<<endl;
	}

}
