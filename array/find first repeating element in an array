#include<iostream>
#include <algorithm>
#include<limits>
// #include<string>
using namespace std;


void printArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}

void inputArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

int repeating(int a[], int n) {

	int max = INT_MIN;
	for(int i = 0; i<n; i++)
		if(max < a[i])
			max = a[i];

	int temp[max + 1] = {};

	for(int i = 0; i<n; i++){
		int num = a[i];
		temp[num]++;
	}

	for(int i = 0; i<n; i++) {
		int num = a[i];
		if(temp[num] > 1)
			return i;
	}

return -1;
}
int main()
{ 
	
    int arr[] = { 10, 5, 3, 4, 6 };
 
    int n = sizeof(arr) / sizeof(arr[0]);
	
	int index = repeating(arr,n);

	if(index != -1)
		cout<<"first repeating element is :  "<< arr[index];
	else 
		cout<<"NO repeating element";
}

