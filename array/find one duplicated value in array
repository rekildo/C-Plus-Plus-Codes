#include<iostream>
#include<limits.h>
using namespace std;





void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}


void sorting(int arr[], int n) {

	int ans = 0;

	for(int i = 0; i<n; i++) {
		ans = ans + arr[i];
	}

	int sum = (n*(n+1))/2;

	cout<<sum-ans;




}



int main()
{	
	int n;
	cout<<"enter first arrays no. of elements : ";
	cin>>n;



	int arr[n];

	
	takeArrInput(arr,n);

	sorting(arr,n);


	// takeArrOutput(arr,n);

	return 0;
}
