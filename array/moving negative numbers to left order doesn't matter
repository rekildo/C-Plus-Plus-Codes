#include<iostream>
#include<limits.h>
using namespace std;





void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}


void sorting(int arr[], int n) {

	int i = 0;
	int j = 0;

	for(int i=0; i<n; i++) {

		if(arr[i]<0) 
		{
			swap(arr[i],arr[j]);
			j++;
		}
	}


}



int main()
{	
	int n;
	cout<<"enter first arrays no. of elements : ";
	cin>>n;



	int arr[n];

	
	takeArrInput(arr,n);

	sorting(arr,n);


	takeArrOutput(arr,n);

	return 0;
}
