#include<iostream>
using namespace std;


void printArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}

void inputArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}


void rotate(int arr[], int n) {

	// int store = arr[n-1];

	// for(int i = n-1; i>0; i--) {
	// 	arr[i] = arr[i-1];
	// }

	// arr[0] = store;
	// return;


	int i = 0;
	int j = n-1;
	while(i<j) {
		swap(arr[i],arr[j]);
	i++;
	}
}


int main()
{
	int arr[] = {1, 2, 3, 4, 5};
	int n  = 5;
	rotate(arr,n);

	printArr(arr,n);
	return 0;
}
