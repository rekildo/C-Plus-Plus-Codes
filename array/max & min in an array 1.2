	}
}


void maximum(int arr[], int size) {

	int maxi = INT_MIN;

	for(int j = 0; j<size; j++) {
		
		maxi = max(arr[j],maxi);
	}

	cout<<"max is : "<< maxi << endl;
}


void minimum(int arr[], int size) {

	int mini = INT_MAX;

	for(int i = 0; i<size; i++) {
		mini = min(arr[i],mini);
	}

	cout<<"min is : "<< mini << endl;
}


