#include<iostream>
#include<algorithm>
using namespace std;

void printArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}

void inputArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}


void Swap(int &a, int  &b) {
	int temp = a;
	a = b;
	b = temp;
}

void ZOT(int arr[], int n) {

	int low = 0;
	int mid = 0; 
	int high = n-1;

	for(int i = 0; i<n; i++) {

		if(arr[mid] == 0) {
			Swap(arr[mid], arr[low]);
			mid++;
			low++;
		}else if(arr[mid] == 1){
			mid++;
		}else{
			Swap(arr[high], arr[mid]);
			high--;
		}

	}

	
}
