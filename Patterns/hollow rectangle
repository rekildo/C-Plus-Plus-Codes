#include<iostream>

using namespace std;

int main()
{	
	int row;
	cin>>row;

	
		for(int i = 1; i<=row; i++) {

			for(int j = 1; j<=row+2;j++ ) {
				if(i == 1 || i == row || j == 1 || j == row+2)
					{	cout<<"* ";
					}else{
						cout<<"  ";
					}
			}
		
		
			cout<<"\n";
		}


	return 0;
}




