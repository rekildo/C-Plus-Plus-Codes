// #include<bits/stdc++.h>
#include<iostream>
#include<algorithm>
using namespace std;



/*Function to find the number of occurrence of a given target
So we just find the first and last occurrence of that number and 
do (last - first + 1) this would give us the number of occurrence of our target*/
 

int binarySearch(int arr[], int target, int elements, int key)
{
	  	int start = 0;
	  	int end = elements-1;

	  	int ans;

	  	while(start <= end) {
	  		int mid = start + (end - start)/2;

	  	if(key == 1) {
		  		if(arr[mid] == target) {	//start of  if block for first element
		  				ans = mid;
		  				end = mid - 1;
		  		}
		  		else if(arr[mid] > target){
		  			end = mid - 1;
		  		}else{
		  			start = mid + 1;
		  		}
	  	 
	  	 }else if(key == 0){						//start of else block for the last element

		  	 	if(arr[mid] == target){
		  	 		ans = mid;
		  	 		start = mid + 1;
		  	 	}else if(arr[mid] > target) {
		  	 		end = mid - 1;
		  	 	}else{
		  	 		start = mid + 1;
		  	 	}
	  	 
	  	 }  //end of if else		 
	  	
	  }//end of while loop	



	return ans;
}
 
int main()
{
    int arr[] = {5,5,5,5,5,9,10,15,17,18};
   		int target;
   		cin>>target;
 		int elements = sizeof(arr)/sizeof(arr[0]);

   		// 



 		int first = binarySearch(arr,target,elements,1);
 		int second = binarySearch(arr,target,elements,0);

 		


 		int result = (second - first) + 1;

 		cout<<"Numbe of OCc:   " << result ;

 
    return 0;
}
