#include<iostream>
using namespace std;

int main()
{	
	//Pyramid Patter 2

	int n;
	cin>>n;

	for(int i = 1; i<=n; i++) {

		if(i == 1 || i == n)
		{
			for(int j = 1; j<=n-i+1; j++) {
			cout<<"* ";
			}
		}else{
			cout<<"* ";
			for(int j = 1; j<=n-i-1; j++) {
				cout<<"  ";
			}
			
			cout<<"* ";
		}

		cout<<endl;
	}


	return 0;
}
