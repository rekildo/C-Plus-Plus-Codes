// #include<bits/stdc++.h>
#include<iostream>
#include<algorithm>
using namespace std;
 

int binarySearch(int arr[], int target, int elements)
{
	  	int start = 0;
	  	int end = elements;
	  	int ans = -1;
	  	while(start <= end) {
	  		int mid = start + (end - start)/2;

	  		if(arr[mid] == target)
	  			{	
	  				ans = mid;
					end = mid - 1;
	  				
	  			}
	  		else if(arr[mid] > target){
	  			end = mid - 1;
	  		}else{
	  			start = mid + 1;
	  		}
	  	}

	return ans;
}
 
int main()
{
    int arr[] = {1,2,3,3,5,5,7,8,9,10,15,17,18};
   		int target;
   		cin>>target;
 		int elements = sizeof(arr)/sizeof(arr[0]);

   		// 



 		cout << "first occ at index:  " << binarySearch(arr,target,elements);
 		

 
    return 0;
}
