#include<iostream>
using namespace std;


void printArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}

void inputArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}


void rotate(int arr[], int n, int k) {

	int temp[n];
	int d = 0;

	for(int i = k; i<n; i++) {
		temp[d] = arr[i];
		d++;
	}


	for(int i = 0; i<k; i++) {
		temp[d] = arr[i];
		d++;
	}


	for(int i = 0; i<n; i++) {
		arr[i] = temp[i];
	}
}


int main()
{
	int arr[] = {1, 2, 3, 4, 5};
	int n  = 5;
	int k = 3;
	rotate(arr,n,k);

	printArr(arr,n);
	return 0;
	
	
	//int arr[] =  {1, 2, 3, 4, 5, 6, 7, 8};
	// int n = 8;
	// int d;
	// cin>>d;
	// reverse(arr,arr+n);
	// reverse(arr,arr+d);
	// reverse(arr+d,arr+n);

	// printArr(arr,n);

	// return 0;
}
