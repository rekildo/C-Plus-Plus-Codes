#include<iostream>
using namespace std;



void inputarr(int arr[], int size) {

	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}


void ouputarr(int arr[], int size) {
		for(int i = 0; i<size; i++) {
		  cout<<arr[i]<<" ";
	}
}


void checking(int arr1[],int size, int arr2[]) {
	for(int i = 0 ; i<size; i++) {
		for(int j = 0 ; j<size; j++) {
			if(arr1[i] == arr2[j])
			{
				cout<<"'Value Matched', The Value is : "<<arr1[i]<<endl;
				return;
			}

		}
	}
	cout<<"NO Match" << endl;
}


int main()
{	
	int arr1[3], arr2[3], arr3[3];

	inputarr(arr1,3);
	inputarr(arr2,3);
	inputarr(arr3,3);


 	checking(arr1,3,arr2);
	checking(arr2,3,arr3);
	checking(arr3,3,arr1);



	return 0;
}
