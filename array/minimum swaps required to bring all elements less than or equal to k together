#include<iostream>
using namespace std;

void MinSwap(int a[], int n, int k) {
	int good = 0;

	for(int i =0; i<n; i++) {
		if(a[i] <= k)
			good++;
	}

	int bad = 0;
	for(int i = 0; i<good; i++) {
		if(a[i] > k)
			bad++;
	}
	int ans = bad;
	for(int i = 0, j = good; j<n; ++i, ++j) {

		if(a[j] > k)
			bad++;

		if(a[i] > k)
			bad--;

		ans = min(ans,bad);
	}

	cout<<"Minimun number of Swap req: "<<ans;
}


int main()
{
	int arr[] ={2, 1, 5, 6, 3};
	int k = 5;
	int size = sizeof(arr)/sizeof(arr[0]);

	MinSwap(arr,size,k);
}
