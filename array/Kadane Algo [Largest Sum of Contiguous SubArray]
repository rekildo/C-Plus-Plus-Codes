#include<iostream>
#include<limits.h>
using namespace std;





void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}

void KadanesAlgo(int arr[], int n) {

	int i = 0;
	int sum = 0;
	int maxi = INT_MIN;
	while(i < n)
	{
		sum = sum + arr[i];

		maxi = max(maxi,sum);

		if(sum<0)
			sum = 0;


		i++;
	}

	cout<<maxi<<endl;

}







int main()
{	
	int n;
	cout<<"enter first arrays no. of elements : ";
	cin>>n;



	int arr[n];

	
	takeArrInput(arr,n);

	KadanesAlgo(arr,n);


	// takeArrOutput(arr,n);

	return 0;
}
