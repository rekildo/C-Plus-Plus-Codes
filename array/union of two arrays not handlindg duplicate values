#include<iostream>
#include<limits.h>
using namespace std;





void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}


void Union(int arr1[],int arr2[], int n, int k) {

	int i = 0;
	int j = 0;
	int count = 0;

	

	while(i<n && j<k) 
	{
		if(arr1[i] == arr2[j])
		{
			cout<<arr1[i++]<<" ";

			j++;
		}else if(arr1[i] > arr2[j]){
				cout<<arr2[j++]<<" ";
				
		}else{
			cout<<arr1[i++]<<" ";
			
		}
		count++;
	}


	

	//printing remaining elements:

	while(i < n )
	{
		cout<<arr1[i++]<<" ";
	}

	while(j < k)
	{
		cout<<arr2[j++]<<" ";
	}
	
}


int main()
{	
	int n;
	cout<<"enter first arrays no. of elements : ";
	cin>>n;

	int k;
	cout<<"enter second arrays no. of elements : ";
	cin>>k;


	int arr1[n];
	int arr2[k];
	
	takeArrInput(arr1,n);
	takeArrInput(arr2,k);



	Union(arr1,arr2,n,k);




	// takeArrOutput(arr,n);

	return 0;
}
