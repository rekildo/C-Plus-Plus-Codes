#include<iostream>
using namespace std;


void reverseArr(int arr[], int size) {
	int i = 0;
	int j = size - 1;

	while(i<j){
		swap(arr[i],arr[j]);
		i++;
		j--;
	}

}



void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}


int main()
{	
	int n;
	cin>>n;

	int arr[n];

	takeArrInput(arr,n);

	reverseArr(arr,n);	

	takeArrOutput(arr,n);

	return 0;
}
