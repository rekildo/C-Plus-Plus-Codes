#include<iostream>
#include<limits.h>
using namespace std;





void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}


void checkingPalindrom(int arr[], int n) {

	int i = 0 ;
	int j = n - 1;

	while(i < j) 
	{
		if(arr[i] == arr[j]){
			i++;
			j--;
		}else{
			cout<<"NOT PALINDROME";
			return;
		}
	}

	cout<<"PALINDROME"<<endl;

}





int main()
{	
	int n;
	cin>>n;

	int arr[n];

	
	takeArrInput(arr,n);

	checkingPalindrom(arr, n);


	// takeArrOutput(arr,n);

	return 0;
}
