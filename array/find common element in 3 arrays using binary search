#include<iostream>
#include <algorithm>
#include<limits>
// #include<string>
using namespace std;


void printArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}

void inputArr(int arr[], int size) {
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

int BinarySeach(int arr[], int n, int s) {

	int l = 0;
	int h = n-1;
	while(l<=h) {
		int mid = (l+h)/2;

		if(arr[mid]==s)
			return true;
		else if(arr[mid] > s) 
			h = mid - 1;
		else
			l = mid + 1;
	}

	return false;

}



void findCommon(int a[], int b[], int c[], int n1, int n2, int n3) {

	for(int i  = 0; i<n1; i++) {

		if(i != 0 && a[i]==a[i-1])
			continue;

		if(BinarySeach(b,n2,a[i]) && BinarySeach(c,n3,a[i]))
			cout<<a[i]<<endl;


	}


}

int main()
{ //for duplicate elements
    int ar1[] = { 1, 5, 10,10,10, 20, 40, 80 };
    int ar2[] = { 6, 7, 20, 80,80, 100 };
    int ar3[] = { 3, 4, 15, 20, 30, 70, 80, 120 };

    int n1 = sizeof(ar1) / sizeof(ar1[0]);
    int n2 = sizeof(ar2) / sizeof(ar2[0]);
    int n3 = sizeof(ar3) / sizeof(ar3[0]);


    cout << "Common Elements are ";
    findCommon(ar1, ar2, ar3, n1, n2, n3);
}

