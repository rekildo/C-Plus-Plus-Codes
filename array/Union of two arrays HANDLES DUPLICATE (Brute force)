#include<iostream>
#include<unordered_map>
using namespace std;

void printArr(int a[], int n) {

	for(int i = 0; i<n; i++) {
		cout<<a[i]<<" ";
	}
}

void Union(int a1[], int a2[], int size1, int size2) {

	int i = 0;
	int j = 0;

	while(i<size1 && j<size2) {

	if((i>0 && a1[i] == a1[i-1] )) {
		i++;
		}else if(j>0 && a2[j] == a2[j-1]) {
			j++;
		} else if(a1[i] == a2[j]){
			cout<<a1[i++]<<" ";
			j++;
		}
		else if(a1[i] > a2[j])
			cout<<a2[j++]<<" ";
		else 
			cout<<a1[i++]<<" ";

	}



	while(i<size1){ 
		if(a1[i] == a1[i-1]){
			i++;
			continue;
		}
		cout<<a1[i++]<<" ";
	}
	while(j<size2){
		if(a2[j] == a2[j-1]){
			j++;
			continue;
		}
		cout<<a2[j++]<<" ";
	}

}

int main()
{
	int arr1[]  = {1,2,2,2,3,3,4,4,5,6,9,9,9};
	int arr2[] = {3,5,7,7,7,8,8,8,9};
	
	int n1 = sizeof(arr1)/sizeof(arr1[0]);
	int n2 = sizeof(arr2)/sizeof(arr2[0]);

	Union(arr1,arr2,n1,n2);
	

}


