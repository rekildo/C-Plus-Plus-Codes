#include<iostream>
#include<limits.h>
using namespace std;





void takeArrInput(int arr[], int size) {
	cout<<"enter arrays elements: " << endl;
	for(int i = 0; i<size; i++) {
		cin>>arr[i];
	}
}

void takeArrOutput(int arr[],int size) {

	for(int i = 0; i<size; i++) {
		cout<<arr[i]<<" ";
	}
}












void count(int arr[], int size) {

	int zero = 0, one = 0, two = 0;

	for(int i = 0; i<size; i++) {
		if(arr[i] == 0)
			zero++;
		else if(arr[i] == 1)
			one++;
		else
			two++;

	}

	int i = 0;
	while(zero--)
	{
		arr[i] = 0;
		i++;
	}

	while(one--)
	{
		arr[i] = 1;
		i++;
	}

	while(two--)
	{
		arr[i] = 2;
		i++;
	}

}



int main()
{	
	int n;
	cin>>n;

	int arr[n];

	
	takeArrInput(arr,n);

	count(arr,n);


	takeArrOutput(arr,n);

	return 0;
}
