#include<iostream>
#include<algorithm>
using namespace std;

void printArr(int a[][3], int m, int n) {
	for(int i = 0; i<m; i++) {
		for(int j = 0; j<n; j++) {
			cout<<a[i][j]<<" ";
		}
		cout<<endl;
	}
}

void linearSearch(int a[][3], int row, int col) {
	
	for(int i = 0; i<row; i++) {
		for(int j = i; j<col; j++) {
			swap(a[i][j],a[j][i]);
		}
	
	}
	
}


void reversee(int a[][3], int m, int n) {

	for(int i = 0; i<m; i++) {
		int start = 0;
		int end = m-1;
		for(int j = 0; j<n/2; j++) {
			swap(a[start++][i],a[end--][i]);
		}
	}
}
int main()
{	
	int arr[3][3] = {
					  {1,2,3},
					  {4,5,6},
					  {7,8,9}
					};
	int n = 3;

	reversee(arr,3,3);

	 linearSearch(arr,3,3);
	
	printArr(arr,3,3);
}
